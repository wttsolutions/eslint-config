{
  "name": "@wttsolutions/eslint-config",
  "version": "0.0.54",
  "main": "config-base.js",
  "scripts": {
    "version:patch": "yarn version --patch --no-git-tag-version",
    "version:minor": "yarn version --minor --no-git-tag-version",
    "version:major": "yarn version --major --no-git-tag-version",
    "publ": "yarn publish --no-git-tag-version --access public",
    "test-base": "yarn --silent eslint --config ./config-base.js ./test/test-base.js |./utils/check_includes.sh '2 problems'",
    "test-react": "yarn --silent eslint --config ./config-react.js ./test/test-react.jsx |./utils/check_includes.sh '2 problems'",
    "test-ts": "yarn --silent eslint --config ./config-ts.js ./test/test-ts.ts |./utils/check_includes.sh '2 problems'",
    "test-tsx": "yarn --silent eslint --config ./config-ts.js ./test/test-tsx.tsx |./utils/check_includes.sh '2 problems'",

    "test": "concurrently yarn:test-*"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.24.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^2.7.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "react": "*"
  },
  "files": [
    "index.js",
    "react.js"
  ],
  "eslintConfig": {},
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wttsolutions/eslint-config.git"
  }
}
